    "version": "0.2.0",
    "configurations": [
        {
        "name": "Debug Rust (CodeLLDB)",
        "type": "lldb",
        "request": "launch",
        "program": "${workspaceFolder}/target/debug/${workspaceFolderBasename}",
        "args": [],
        "cwd": "${workspaceFolder}",
        "env": {
            "RUST_BACKTRACE": "1"
        },
        "sourceLanguages": ["rust"],
        "preLaunchTask": "rust: cargo build",
        "terminal": "integrated"
        }
    ]
}